<Style name="xline style 2">
  <Rule>
    &maxscale_zoom13;
    <LineSymbolizer stroke="#a1cbea" stroke-linejoin="round" stroke-width="13.0" />
  </Rule>
  <Rule>
    &minscale_zoom12;
    &maxscale_zoom11;
    <LineSymbolizer stroke="#a1cbea" stroke-linejoin="round" stroke-width="9.0" />
  </Rule>
</Style>
<Layer name="xlayer 4" status="on">
  <StyleName>xline style 2</StyleName>
  <Datasource>
    <Parameter name="type">shape</Parameter>
    <Parameter name="file">&coastlines;</Parameter>
  </Datasource>
</Layer>

