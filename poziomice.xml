<Style name="contours10" opacity="0.5">
        <Rule>
                <MaxScaleDenominator>25000</MaxScaleDenominator>
                <MinScaleDenominator>5000</MinScaleDenominator>
                <LineSymbolizer
                        stroke="#995353"
                        stroke-width="0.5"
                />
        </Rule>
</Style>

<Style name="contours20" opacity="0.5">
        <Rule>
                <MaxScaleDenominator>50000</MaxScaleDenominator>
                <MinScaleDenominator>25000</MinScaleDenominator>
                <LineSymbolizer
                        stroke="#995353"
                        stroke-width="0.5"
                        stroke-linejoin="round"
                />
        </Rule>
</Style>

<Style name="contours50" opacity="0.5">
        <Rule>
                <MaxScaleDenominator>200000</MaxScaleDenominator>
                <MinScaleDenominator>50000</MinScaleDenominator>
                <LineSymbolizer
                        stroke="#995353"
                        stroke-width="0.6"
                        stroke-linejoin="round"
                />
        </Rule>
        <Rule>
                <MaxScaleDenominator>25000</MaxScaleDenominator>
                <MinScaleDenominator>5000</MinScaleDenominator>
                <LineSymbolizer
                        stroke="#995353"
                        stroke-width="0.6"
                        stroke-linejoin="round"
                />
                <TextSymbolizer face-name="DejaVu Sans Book" size="11" fill="#995353" halo-radius="1" placement="line" max-char-angle-delta="40">[elev2]</TextSymbolizer>
        </Rule>
</Style>
<Style name="contours100" opacity="0.5">
        <Rule>
                <MaxScaleDenominator>200000</MaxScaleDenominator>
                <MinScaleDenominator>50000</MinScaleDenominator>
                <LineSymbolizer
                        stroke="#995353"
                        stroke-width="1"
                        stroke-linejoin="round"
                />
                <TextSymbolizer face-name="DejaVu Sans Book" size="11" fill="#995353" halo-radius="1" placement="line" max-char-angle-delta="40">[elev2]</TextSymbolizer>
        </Rule>
        <Rule>
                <MaxScaleDenominator>50000</MaxScaleDenominator>
                <MinScaleDenominator>5000</MinScaleDenominator>
                <LineSymbolizer
                        stroke="#995353"
                        stroke-width="1.2"
                        stroke-linejoin="round"
                />
                <TextSymbolizer face-name="DejaVu Sans Book" size="11" fill="#995353" halo-radius="1" placement="line" max-char-angle-delta="40">[elev2]</TextSymbolizer>
        </Rule>
</Style>


<Layer name="contours_10">
    <StyleName>contours10</StyleName>
    <Datasource>
      &datasource-settings-contour;
<Parameter name="table">(select the_geom,elev from contour WHERE elev::integer % 10 = 0 AND ((elev::integer % 50 != 0 AND elev::integer % 100 != 0) OR elev::integer = 0)) as "contours-10"</Parameter>
<Parameter name="geometry_field">the_geom</Parameter>
    </Datasource>
  </Layer>   
  
  <Layer name="contours_20">
    <StyleName>contours20</StyleName>
    <Datasource>
      &datasource-settings-contour;
<Parameter name="table">(select the_geom,elev from contour WHERE elev::integer % 20 = 0 AND elev::integer % 100 != 0) as "contours-20"</Parameter>
<Parameter name="geometry_field">the_geom</Parameter>
    </Datasource>
  </Layer>   
  
<Layer name="contours_50">
    <StyleName>contours50</StyleName>
    <Datasource>
      &datasource-settings-contour;
<Parameter name="table">(select the_geom,elev from contour WHERE elev::integer % 50 = 0 AND elev::integer % 100 != 0) as "contours-50"</Parameter>
<Parameter name="geometry_field">the_geom</Parameter>
    </Datasource>
  </Layer>   
  
  <Layer name="contours_100">
    <StyleName>contours100</StyleName>
    <Datasource>
      &datasource-settings-contour;
<Parameter name="table">(select the_geom,elev from contour WHERE elev::integer % 100 = 0 AND elev::integer != 0) as "contours-100"</Parameter>
<Parameter name="geometry_field">the_geom</Parameter>
    </Datasource>
  </Layer>
